    <style name="AppBottomNavStyle" parent="Widget.Material3.BottomNavigationView">
        <item name="android:background">@color/nav_background</item>
        <item name="itemIconTint">@color/nav_item_color</item>
        <item name="itemTextColor">@color/nav_item_color</item>
        <item name="labelVisibilityMode">labeled</item>
    </style>
    <style name="AppButtonStyle" parent="Widget.Material3.Button">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="cornerRadius">12dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="backgroundTint">@color/primary_color</item>
    </style>
    <style name="AppButtonStyle.Outlined" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="cornerRadius">12dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="strokeColor">@color/primary_color</item>
        <item name="android:textColor">@color/primary_color</item>
    </style>
    <style name="AppButtonStyle.Primary" parent="AppButtonStyle">
        <item name="backgroundTint">@color/primary_color</item>
        <item name="android:textColor">@color/text_white</item>
    </style>
    <style name="AppButtonStyle.Submit" parent="AppButtonStyle">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">56dp</item>
        <item name="cornerRadius">16dp</item>
        <item name="backgroundTint">@color/accent_color</item>
    </style>
    <style name="AppCardStyle" parent="Widget.Material3.CardView.Elevated">
        <item name="cardCornerRadius">12dp</item>
        <item name="cardElevation">4dp</item>
        <item name="cardUseCompatPadding">true</item>
        <item name="cardBackgroundColor">@color/background_card</item>
    </style>
    <style name="AppCardStyle.Submit" parent="AppCardStyle">
        <item name="cardBackgroundColor">@color/background_submit</item>
        <item name="cardCornerRadius">16dp</item>
        <item name="cardElevation">6dp</item>
    </style>
